./dm attach $1:$2:120
./dm setsessionservice $1:$2:"":true:300
admin_token=`./dm logindevice $1:$2:$5:$6| cut -d' ' -f4`
./dm createaccount $1:$2:$admin_token:admin_1:admin_1:Administrator
./dm createaccount $1:$2:$admin_token:operator:operator:Operator
./dm createaccount $1:$2:$admin_token:readonly:readonly:ReadOnlyUser
admin_1_token=`./dm logindevice $1:$2:admin_1:admin_1| cut -d' ' -f4`
operator_token=`./dm logindevice $1:$2:operator:operator| cut -d' ' -f4`
readonly_token=`./dm logindevice $1:$2:readonly:readonly| cut -d' ' -f4`
./dm createaccount $1:$2:$operator_token:admin_2:admin_2:Administrator
./dm createaccount $1:$2:$readonly_token:admin_2:admin_2:Administrator
./dm createaccount $1:$2:$readonly_token:operator_1:operator_1:Operator
./dm deleteaccount $1:$2:$operator_token:admin_1
./dm deleteaccount $1:$2:$readonly_token:operator
./dm deleteaccount $1:$2:$operator_token:operator
./dm deleteaccount $1:$2:$readonly_token:readonly
./dm deleteaccount $1:$2:$admin_token:admin_1
./dm deleteaccount $1:$2:$admin_token:operator
./dm deleteaccount $1:$2:$admin_token:readonly
./dm detach $1:$2:$admin_token

sed -e '/^\/\//d' -e 's/ip1/'${1}'/g; s/port1/'${2}'/g; s/ip2/'${3}'/g; s/port2/'${4}'/g; s/admin_token/'${token}'/g' "$9".expected > "$9".e
