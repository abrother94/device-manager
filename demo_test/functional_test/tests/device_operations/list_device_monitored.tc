./dm showdevices
./dm attach $1:$2:120 $3:$4:100
./dm setsessionservice $1:$2:"":true:300
token_1=`./dm logindevice $1:$2:$5:$6| cut -d' ' -f4`
./dm setsessionservice $3:$4:"":true:300
token_2=`./dm logindevice $3:$4:$7:$8| cut -d' ' -f4`
ret=`./dm showdevices`
echo $ret
Ip1=`echo $ret|awk -F[:\"\ ] '{print $1}'`
Ip2=`echo $ret|awk -F[:\"\ ] '{print $3}'`
Port1=`echo $ret|awk -F[:\"\ ] '{print $2}'`
Port2=`echo $ret|awk -F[:\"\ ] '{print $4}'`
if [ "${Ip1}" == "${3}" -a "${Ip2}" == "${1}" ] && [ "${Port1}" == "${4}" -a "${Port2}" == "${2}" ]; then
    Ip1=${3}
    Port1=${4}
    Ip2=${1}
    Port2=${2}
fi
./dm detach $1:$2:$token_1
./dm detach $3:$4:$token_2

sed -e '/^\/\//d' -e 's/ip1/'${1}'/g; s/port1/'${2}'/g; s/ip2/'${3}'/g; s/port2/'${4}'/g; s/token/'${token}'/g; s/Ip1/'${Ip1}'/g; s/Port1/'${Port1}'/g; s/Ip2/'${Ip2}'/g; s/Port2/'${Port2}'/g' "$9".expected > "$9".e
