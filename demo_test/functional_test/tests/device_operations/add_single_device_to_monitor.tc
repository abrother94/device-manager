./dm attach $1:$2:120
./dm setsessionservice $1:$2:"":true:300
token=`./dm logindevice $1:$2:$5:$6| cut -d' ' -f4`
./dm attach $1:88:120
./dm attach $1:88
./dm detach $1:$2:$token

sed -e '/^\/\//d' -e 's/ip1/'${1}'/g; s/port1/'${2}'/g; s/ip2/'${3}'/g; s/port2/'${4}'/g; s/token/'${token}'/g' "$9".expected > "$9".e
